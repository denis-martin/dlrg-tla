				},
				"sex": {
					"type": [ "string", "null" ]
				},
				"street": {
					"type": [ "string", "null" ]
				},
				"zipCode": {
					"type": [ "string", "null" ]
				},
				"city": {
					"type": [ "string", "null" ]
				},
